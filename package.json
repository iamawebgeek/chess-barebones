{
  "name": "chess-barebones-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo to manage chess-barebones project",
  "author": "iamawebgeek <iamawebgeek@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamawebgeek/chess-barebones.git"
  },
  "scripts": {
    "changeset": "changeset",
    "docs:dev": "pnpm --dir docs start",
    "docs:build": "pnpm --dir docs build",
    "docs:serve": "pnpm --dir docs serve",
    "docs:clear": "pnpm --dir docs clear",
    "docs:deploy": "pnpm --filter chess-barebones-docs deploy",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-workspace-scopes": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "simple-git-hooks": "^2.13.1",
    "tsup": "^8.5.0",
    "turbo": "^2.5.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@changesets/cli": "^2.29.6"
  }
}
